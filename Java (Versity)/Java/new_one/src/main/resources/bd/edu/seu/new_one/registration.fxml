      </Label>
      <Label layoutX="213.0" layoutY="76.0" text="Name" />
      <TextField fx:id="nameField" layoutX="213.0" layoutY="100.0" />
      <TextField fx:id="emailField" layoutX="213.0" layoutY="159.0" />
      <Label layoutX="215.0" layoutY="136.0" text="Mail" />
      <PasswordField fx:id="passwordField" layoutX="213.0" layoutY="215.0" />
      <Button layoutX="256.0" layoutY="255.0" mnemonicParsing="false" onAction="#registerEvent" text="Sign Up" />
      <Label layoutX="215.0" layoutY="192.0" text="Password" />
      <Label layoutX="263.0" layoutY="303.0" onMouseClicked="#goToLoginEvent" text="Login" textFill="#2200ff" underline="true">
         <font>
            <Font size="15.0" />
